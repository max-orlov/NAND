/**
 * 8-way 16-bit multiplexor:
 * out = a if sel == 000
 *       b if sel == 001
 *       c if sel == 010
 *		 d if sel == 011
 * 		 e if sel == 100
 *		 f if sel == 101
 * 		 g if sel == 110
 *       h if sel == 111
 */

CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
	
	Mux16(a=a,b=e,sel=sel[2],out=outMux-11);
	Mux16(a=c,b=g,sel=sel[2],out=outMux-12);
	Mux16(a=outMux-11,b=outMux-12,sel=sel[1],out=outMux-1);

	Mux16(a=b,b=f,sel=sel[2],out=outMux-21);
	Mux16(a=d,b=h,sel=sel[2],out=outMux-22);
	Mux16(a=outMux-21,b=outMux-22,sel=sel[1],out=outMux-2);
	
	Mux16(a=outMux-1,b=outMux-2,sel=sel[0],out=out);	
}